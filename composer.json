{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "alajusticia/laravel-auth-tracker": "^2.1",
        "darkaonline/l5-swagger": "^8.3",
        "elasticsearch/elasticsearch": "^8.4",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/framework": "9.52.4",
        "laravel/passport": "^10.3",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.8",
        "league/flysystem": "3.8.0",
        "ramsey/uuid": "4.7.0",
        "spiral/roadrunner-laravel": "^5.11",
        "torann/geoip": "^3.0",
        "whichbrowser/parser": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.15",
        "laravel/sail": "^1.0.1",
        "laravel/telescope": "^4.14",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpunit": "@php ./vendor/bin/phpunit -d memory_limit=512M",
        "phpstan": "@php -d 'opcache.enable=0' ./vendor/bin/phpstan analyze -c ./phpstan.neon --no-progress --ansi",
        "phpstan-xdebug": "@php -d 'opcache.enable=0' ./vendor/bin/phpstan analyze -c ./phpstan.neon --no-progress --ansi --xdebug",
        "check-style": "./vendor/bin/php-cs-fixer fix -vvv --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots",
        "test": [
          "@phpstan",
          "@phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
